<div id="posts-container">
  <% @presenter.all.each do |post| %>
    <%= render partial: 'posts/content', locals: { post: post } %>
  <% end %>
</div>

<%= link_to "More", root_path(page: 2), id: 'more-btn', remote: true, class: "hidden" %>

<%= javascript_pack_tag 'controllers/post_controller' %>
<%= javascript_pack_tag 'plugins/init_swiper' %>
